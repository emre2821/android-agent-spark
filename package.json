{
  "name": "android-agent-spark",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "python -m app.cli runserver --host 0.0.0.0 --port 8000",
    "dev:frontend": "npm --prefix web run dev",
    "build:web": "npm --prefix web run build",
    "lint:web": "npm --prefix web run lint",
    "install:web": "npm --prefix web install"
  },
  "dependencies": {
    "cron-parser": "^5.4.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "vitest": "^1.6.1"
  }
}
