{
  "name": "agent-spark-monorepo",
  "version": "2024.11.7",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:frontend": "npm --prefix apps/frontend-app run dev",
    "dev:web": "npm --prefix apps/web-app run dev",
    "dev:server": "npm --prefix services/nodejs-server run dev",
    "dev": "concurrently \"npm:dev:web\" \"npm:dev:server\"",
    "build:frontend": "npm --prefix apps/frontend-app run build",
    "build:web": "npm --prefix apps/web-app run build",
    "build:server": "npm --prefix services/nodejs-server run build",
    "build": "npm run build:web && npm run build:server",
    "test": "npm run test:web && npm run test:server",
    "test:frontend": "npm --prefix apps/frontend-app run test",
    "test:web": "npm --prefix apps/web-app run test",
    "test:server": "npm --prefix services/nodejs-server run test",
    "lint": "npm run lint:web && npm run lint:server && npm run lint:frontend",
    "lint:frontend": "npm --prefix apps/frontend-app run lint || echo \"no lint script in /apps/frontend-app\"",
    "lint:web": "npm --prefix apps/web-app run lint || echo \"no lint script in /apps/web-app\"",
    "lint:server": "npm --prefix services/nodejs-server run lint || echo \"no lint script in /services/nodejs-server\"",
    "dev:backend": "cd services/python-backend && python -m app.cli runserver --host 0.0.0.0 --port 8000",
    "build:mobile": "node scripts/build/build_mobile.js",
    "build:desktop": "node scripts/build/build_desktop.js",
    "typecheck": "npm exec --prefix apps/web-app tsc -- --project apps/web-app/tsconfig.json --noEmit",
    "test:e2e": "cd services/python-backend && python -m pytest tests/concurrency",
    "install:all": "npm install && npm --prefix apps/frontend-app install && npm --prefix apps/web-app install && npm --prefix services/nodejs-server install",
    "validate": "npm run lint && npm run typecheck && npm test"
  },
